{
    "name": "hapiwork/nova-react",
    "description": "Building Laravel Nova custom field with your Reactjs skill",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Hapiwork\\NovaReact\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hapiwork\\NovaReact\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
